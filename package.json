{
  "dependencies": {
    "expo": "^54.0.1",
    "expo-asset": "~12.0.8",
    "expo-file-system": "~19.0.11",
    "expo-gl": "~16.0.6",
    "expo-modules-core": "~3.0.15",
    "expo-three": "^2.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "three": "^0.145.0"
  },
  "name": "rscubes-expo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "npm run prepare-web && expo start --web",
    "bundle": "node scripts/bundle-html.js",
    "prepare-web": "mkdir -p public && cp assets/webapp/index.html public/ && cp assets/webapp/renderer.bundle.js public/ && cp -r assets/webapp/textures public/ 2>/dev/null || true"
  },
  "devDependencies": {
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  }
}
